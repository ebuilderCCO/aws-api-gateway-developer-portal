{
    "Region": "eu-west-1", 
    "Grants": [
        {
            "Type": "Group", 
            "URI": "http://acs.amazonaws.com/groups/global/AllUsers", 
            "Permission": "READ"
        }
    ], 
    "Policy": {
        "Version": "2012-10-17", 
        "Statement": [
            {
                "Sid": "Allow Public Access to All Objects", 
                "Effect": "Allow", 
                "Principal": "*", 
                "Action": "s3:GetObject", 
                "Resource": "arn:aws:s3:::ebuilder-devportal-@account@.ebuilder.io/*"
            }
        ]
    }, 
    "WebsiteConfig": {
				"ErrorDoc": "error.html",
        "IndexDoc": "index.html"
    }
}
